        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="JoinContactActivityTheme" parent="@style/ContactPickerTheme">
        <item name="disabled_background">@drawable/list_item_disabled_background</item>
    </style>
    <style name="ContactsPreferencesTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:listViewStyle">@style/ListViewStyle</item>
    </style>
